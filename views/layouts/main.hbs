                  </ul>
                </nav>
                <div class='mobile-button'><span></span></div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </header>
    <!-- end Header -->

    {{{body}}}

    <!-- Footer -->
    <footer class='footer style-2'>
      <div class='footer__top'>
        <div class='container'>
          <div class='row'>
            <div class='col-md-12'>
              <div class='content'>
