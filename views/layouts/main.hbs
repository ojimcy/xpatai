    <a id='scroll-top'></a>

    <script src='/js/jquery.min.js'></script>
    <script src='/js/jquery.easing.js'></script>
    <script src='/js/app.js'></script>
    <script src='/js/count-down.js'></script>
    <script src='/js/aos.js'></script>
    <script src='/js/chart.js'></script>
    <script>
      AOS.init();
    </script>

    <script>
      let numberPercent = document.querySelectorAll('.countbar'); let getPercent
      = Array.from(numberPercent); getPercent.map((items) => { let startCount =
      0; let progressBar = () => { startCount++; items.style.width =
      `${startCount}%`; if (startCount == items.dataset.percentnumber) {
      clearInterval(stop); } }; let stop = setInterval(() => { progressBar(); },
      25); });
    </script>

    <script src='/js/mouse.js'></script>
  </body>

</html>